{
  "name": "mastermind",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "server:debug": "nodemon --inspect app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "play": "concurrently \"npm start\" \"npm run frontend\"",
    "dev:debug": "concurrently \"npm run server:debug\" \"npm run frontend\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "d3": "^5.15.0",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "gh-pages": "^2.2.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "react-router-dom": "^5.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^8.0.2"
  }
}
